{
  "name": "electron-dev-webpack-plugin",
  "version": "1.0.0",
  "description": "A webpack plugin for electron development",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "dev": "tsc -p ./tsconfig.dev.json && node lib/webpack.config.js",
    "build": "tsc -p ./tsconfig.json",
    "lint": "tslint -p ./tsconfig.json",
    "release": "npm run lint && npm run build && npm publish"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "portfinder": "^1.0.20"
  },
  "peerDependencies": {
    "electron": "^2.0.14"
  },
  "devDependencies": {
    "@types/webpack": "^4.4.25",
    "electron": "^2.0.14",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nashaofu/electron-dev-webpack-plugin.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "webpack",
    "plugin",
    "electron",
    "development"
  ],
  "author": "nashaofu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nashaofu/electron-dev-webpack-plugin/issues"
  },
  "homepage": "https://github.com/nashaofu/electron-dev-webpack-plugin#readme"
}
